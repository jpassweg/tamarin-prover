theory MessageFillComposition
begin

builtins: homomorphic-encryption

rule GetKey:
  [ Fr(~k) ]
  --[]->
  [ !Key(~k) ]

rule A:
  [ Fr(~x0)
  , !Key(k) ]
  --[]->
  [ Out(henc(~x0, k)) ]

rule B:
  [ !Key(k)
  , In(henc(<x0, x1>, k)) ]
  --[ Received(k) ]->
  []

lemma Intruder_Fills_Message: exists-trace
  "Ex k #i. Received(k) @i & not K(k) @i"

end