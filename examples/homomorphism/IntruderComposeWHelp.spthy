theory IntruderComposeWithHelper
begin

builtins: homomorphic-encryption

rule GetKey:
  [ Fr(~k) ]
  --[]->
  [ !Key(~k) ]

rule A:
  [ Fr(~x0)
  , !Key(k) ]
  --[]->
  [ Out(henc(~x0, k)) ]

rule Helper:
  [ In(henc(x0, k))
  , In(henc(x1, k)) ]
  --[]->
  [ Out(henc(<x0, x1>, k)) ]

// Does not use Helper
// Found trace in HomomorphicExamples2
// Found trace in HomomorphicExamples3
lemma Intruder_Fills_Message: exists-trace
  "Ex x0 x1 k #i. K(henc(<x0, x1>, k)) @i & not K(k) @i"

end