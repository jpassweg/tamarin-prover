theory IntruderDecompositionThree
begin

builtins: homomorphic-encryption

rule GetKey:
  [ Fr(~k) ]
  --[]->
  [ !Key(~k) ]

rule A:
  [ Fr(~x0)
  , Fr(~x1)
  , !Key(k) ]
  --[ Send(~x0, k) ]->
  [ Out(henc(<~x0, ~x1>, k)) ]

lemma Intruder_Decomposes_Message: exists-trace
  "Ex x0 k #i #j. Send(x0,k) @j & K(henc(x0,k)) @i & not K(k) @i"

end