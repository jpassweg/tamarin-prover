theory IntruderDecomposition
begin

builtins: homomorphic-encryption

rule GetKey:
  [ Fr(~k) ]
  --[]->
  [ !Key(~k) ]

rule A:
  [ Fr(~x0)
  , Fr(~x1)
  , !Key(k) ]
  --[]->
  [ Out(henc(<~x0, ~x1>, k)) ]

// Intruder_Decomposes_Message (exists-trace): verified (3 steps)
lemma Intruder_Decomposes_Message: exists-trace
  "Ex x0 k #i. K(henc(x0,k)) @i & not K(k) @i"

end