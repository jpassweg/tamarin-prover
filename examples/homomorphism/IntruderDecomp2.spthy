theory IntruderDecompositionTwo
begin

builtins: homomorphic-encryption

rule GetKey:
  [ Fr(~k) ]
  --[]->
  [ !Key(~k) ]

rule A1:
  [ Fr(~x0)
  , Fr(~x1)
  , Fr(~x2)
  , !Key(k) ]
  --[]->
  [ Out(<henc(<~x0, ~x1>, k), ~x2>) ]

/*
rule B1:
