theory MessageFillDecomposition
begin

builtins: homomorphic-encryption

rule GetKey:
  [ Fr(~k) ]
  --[]->
  [ !Key(~k) ]

rule A1:
  [ Fr(~x0)
  , Fr(~x1)
  , !Key(k) ]
  --[]->
  [ Out(henc(<~x0, ~x1>, k)) ]

rule B1:
  [ !Key(k)
  , In(henc(x0, k)) ]
  --[ Received(k) ]->
  []

lemma Intruder_Fills_Message: exists-trace
  "Ex k #i . Received(k) @i & not K(k) @i"

end