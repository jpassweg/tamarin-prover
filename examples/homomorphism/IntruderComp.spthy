theory IntruderComposition
begin

builtins: homomorphic-encryption

rule GetKey:
  [ Fr(~k) ]
  --[]->
  [ !Key(~k) ]

rule A:
  [ Fr(~x0)
  , !Key(k) ]
  --[]->
  [ Out(henc(~x0, k)) ]

// Intruder_Composes_Message (exists-trace): verified (4 steps)
lemma Intruder_Composes_Message: exists-trace
  "Ex x0 x1 k #i. K(henc(<x0, x1>, k)) @i & not K(k) @i"

end