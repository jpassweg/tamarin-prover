theory MessageFillDecompose
begin

builtins: homomorphic-encryption

rule GetKey:
  [ Fr(~k) ]
  --[]->
  [ !Key(~k) ]

rule A:
  [ Fr(~x0)
  , Fr(~x1)
  , !Key(k) ]
  --[ Send(~x0, k) ]->
  [ Out(henc(hpair(~x0, ~x1), k)) ]

rule B:
  [ !Key(k)
  , In(henc(x0, k)) ]
  --[ Received(x0, k) ]->
  []

// Need Send(~x0, k) fact or intruder could just set x0 := hpair(~x0, ~x1)
lemma Intruder_Fills_Message: exists-trace
  "Ex x0 k #i #j. Send(x0, k) @i & Received(x0, k) @j & not K(k) @j"

end