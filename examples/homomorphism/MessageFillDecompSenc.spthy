theory MessageFillDecompositionWithSymmetricEncryption
begin

builtins: symmetric-encryption

rule GetKey:
  [ Fr(~k) ]
  --[]->
  [ !Key(~k) ]

rule A:
  [ Fr(~x0)
  , Fr(~x1)
  , !Key(k) ]
  --[ Send(~x0, ~x1, k) ]->
  [ Out(senc(<~x0, ~x1>, k)) ]

rule B:
  [ !Key(k)
  , In(senc(x0, k)) ]
  --[ Received(x0, k) ]->
  []

lemma Intruder_Fills_Message: exists-trace
  "Ex x0 x1 k #i #j. Send(x0, x1, k) @i & Received(x0, k) @j & not K(k) @j"

end